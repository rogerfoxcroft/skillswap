{
  "version": 2,
  "builds": [
    {
      "src": "SkillSwapWeb/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/SkillSwapWeb/dist/$1"
    }
  ],
  "buildCommand": "cd SkillSwapWeb && npm run build",
  "outputDirectory": "SkillSwapWeb/dist",
  "installCommand": "cd SkillSwapWeb && npm install"
}